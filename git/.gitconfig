[alias]
	st = status
	cma = commit --all -m
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	nuke = reset --hard origin/master
	alias = config --get-regexp ^alias\\.
[credential]
	helper = osxkeychain
